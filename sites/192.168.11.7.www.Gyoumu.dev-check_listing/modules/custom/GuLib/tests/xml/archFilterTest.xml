<?xml version="1.0" encoding="UTF-8"?>
<tree name="packing_list" model="stock.picking" string="Packing list" criteria="[[&quot;state&quot;, &quot;=&quot;, &quot;assigned&quot;]]">

  <param name="class">print</param>
  <param name="merge">1</param>  
  <param name="copy_method">ditto</param>
  <param name="count">1</param>
  <param name="extra_info">1</param>

  <field name="date">
    <param name="hide">1</param>
  </field>
  
  <field name="name" string="Ref."/>
  <field name="origin">
    <param name="hide">1</param>
  </field>

  <field name="x_tnum" string="Track #"/>
  <field name="move_lines">
    <field name="product_id">
      <field name="default_code" string="SKU"/>
    </field>
    <field name="product_qty" string="Qty"/>
    <field name="note" string="Remark"/>
  </field>
  
  <field name="address_id" select="1">
    <field name="name" string="Buyer"/>
    <field name="state_id">
      <field name="country_id" string="Country"/>
    </field>
  </field>
  
  <field name="carrier_id" string="Carrier"/>
  
  <translate>
    <function>
      <source name="func:appendChkCode"/>
      
      <field>name</field>
    </function>
  </translate>
  
  <filter>
    <func>
      <src name="func:pregMatch">
        <arg>/Fedex IE|Fedex IP/</arg>
      </src>
      
      <fld>carrier_id</fld>
    </func>

    <func>
      <src name="func:beforeCloseTime"/>
      <fld>date</fld>
    </func>
  </filter>
</tree>