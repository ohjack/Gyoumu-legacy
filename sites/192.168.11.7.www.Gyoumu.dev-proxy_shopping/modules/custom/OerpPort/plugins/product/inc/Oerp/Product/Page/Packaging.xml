<?xml version="1.0"?>
<tree name="view" string="Packaging">
  <param name="oerp:model">product.product</param>
  <param name="oerp:criteria">[["state", "&lt;&gt;", "obsolete"]]</param>

  <field name="product_manager" string="P.Mgr"/>
  <field name="default_code" string="SKU"/>
  <field name="name" string="Name"/>
  <field name="state" string="Status"/>

  <field name="packaging" exclude="0">
    <field name="ul">
      <field name="name" string="Packaging"/>
    </field>
    <field name="length" string="L"/>
    <field name="width" string="W"/>
    <field name="height" string="H"/>
    <field name="weight" string="Wt"/>
  </field>

  <translate>
    <func>
      <src name="cmd:Oerp\Product\Command\overPkgDim"/>
      <fld>packaging.length</fld>
      <fld>packaging.width</fld>
      <fld>packaging.height</fld>
    </func>
  </translate>
</tree>
